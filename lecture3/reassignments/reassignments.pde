// Variable Assignments
int x = 5;
int y = 10;
println("x = " + x);
println("y = " + y);
// Change values
x = x + 5;
y = 7;
println("x = " + x);
println("y = " + y);
// Increment and Decrement Values
x++; // increment x
y--; // decrement y
println("x = " + x);
println("y = " + y);
// Special operators
y -= 4;
x += y;
println("x = " + x);
println("y = " + y);
